{
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true
    },
    "extends": "eslint:recommended",
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
     
        "linebreak-style": [
            "error",
            "windows"
        ],
 
        "semi": [
            "error",
            "always"
        ],
        "no-unused-vars": ["error", { "args": "none" }]
    }
}